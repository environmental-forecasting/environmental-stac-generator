{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "basic": {
          "format": "%(message)s"
      },
      "simple": {
          "format": "%(levelname)s - %(message)s"
      },
      "standard": {
          "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      },
      "detailed": {
          "format": "%(asctime)s - %(name)s - %(levelname)s - %(module)s - %(lineno)d - %(message)s"
      },
      "rich detailed": {
          "format": "%(name)s - %(message)s"
      }
    },
      "handlers": {
        "console_handler": {
          "class": "logging.StreamHandler",
          "level": "NOTSET",
          "formatter": "simple"
        },
        "rich_handler": {
          "class": "rich.logging.RichHandler",
          "formatter": "basic"
        }
      },
    "loggers": {
        "": {
          "level": "WARNING",
          "handlers": ["rich_handler"]
        },
        "environmental_stac_generator.preprocess": {
          "level": "DEBUG",
          "propagate": false,
          "handlers": ["rich_handler"]
        },
        "environmental_stac_generator": {
          "level": "DEBUG",
          "propagate": false,
          "handlers": ["rich_handler"]
        }
    }
}